    <div class="padded text-center">
      <h2 class="section-header">Our team</h2>
      <br>
      <% names = ["Anne", "Romain", "Seb"] %>
      <div class="text-center">
        <div class="container">
          <div class="row">
            <% names.each do |name| %>
            <div class="col-xs-12 col-sm-4 col-md-4">
              <%= image_tag "http://placehold.it/150x150", class: "img-circle" %>
              <h3><%= "#{name}" %></h3>
            </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
